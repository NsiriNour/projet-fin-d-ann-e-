        if not numeric_columns:
            return html.P("Aucune colonne num√©rique disponible pour la visualisation.", style={"color": "gray"}), f"Fichier charg√©: {filename}" if filename else "Fichier charg√©: Aucun", temp_data, 'imputation'

        return html.Div([
            html.Div(id='search-container', style={"display": "none"}),
