                html.P("Réponse :", style={
                    'color': '#FBE4D8',
                    'fontWeight': 'bold',
                    'marginBottom': '5px'
                }),
                html.P(faq_data[tab_value][questions[question_index]], style={
                    'color': 'white',
                    'padding': '15px',
                    'backgroundColor': 'rgba(255, 255, 255, 0.1)',
                    'borderRadius': '5px',
                    'borderLeft': '4px solid #00ffcc'
                })
            ])
        return ""
