@app.callback(
    [Output('test-results', 'children'),
     Output('download-test-btn', 'disabled'),
     Output('test-message', 'children'),
     Output('test-results-store', 'data')],
    [Input('run-test-btn', 'n_clicks')],
    [State('test-type', 'value'),
     State('test-column', 'value'),
     State('group-column', 'value'),
     State('value-column', 'value'),
     State('chi2-column1', 'value'),
     State('chi2-column2', 'value'),
     State('temp-data-store', 'data')]
)
def run_statistical_test(n_clicks, test_type, test_column, group_column, value_column, chi2_column1, chi2_column2, temp_data):
    if not n_clicks or not test_type or not temp_data:
        return dash.no_update, dash.no_update, dash.no_update, dash.no_update

    df_temp = pd.DataFrame.from_dict(temp_data)
    if df_temp.empty:
        return html.P("Erreur : Aucune donn√©e disponible.", style={"color": "red"}), True, "Erreur : Aucune donn√©e disponible.", None

    results = {}
    try:
        # V√©rifier les colonnes num√©riques et cat√©goriques
        numeric_columns = [col for col in df_temp.columns if pd.to_numeric(df_temp[col], errors='coerce').notna().mean() > 0.5]
        categorical_columns = [col for col in df_temp.columns if col not in numeric_columns]

        if test_type == 't-test':
            if not group_column or not value_column:
                return html.P("Veuillez s√©lectionner une colonne de regroupement et une colonne de valeurs num√©riques.", style={"color": "red"}), True, "Veuillez s√©lectionner les colonnes n√©cessaires.", None
            if value_column not in numeric_columns:
                return html.P("Erreur : La colonne des valeurs doit contenir des donn√©es num√©riques.", style={"color": "red"}), True, "La colonne des valeurs doit √™tre num√©rique.", None
            groups = df_temp.groupby(group_column)[value_column].apply(lambda x: x.dropna().tolist())
            if len(groups) != 2:
                return html.P("Erreur : Le t-test n√©cessite exactement 2 groupes.", style={"color": "red"}), True, "Le t-test n√©cessite 2 groupes.", None
            if any(len(g) < 2 for g in groups):
                return html.P("Erreur : Chaque groupe doit contenir au moins 2 observations.", style={"color": "red"}), True, "Chaque groupe doit avoir au moins 2 observations.", None
            stat, p_value = ttest_ind(groups.iloc[0], groups.iloc[1])
            results = {
                "Test": "t-test",
                "Statistique": stat,
                "p-valeur": p_value,
                "Interpr√©tation": "Diff√©rence significative entre les groupes" if p_value < 0.05 else "Pas de diff√©rence significative"
            }

        elif test_type == 'normality':
            if not test_column:
                return html.P("Veuillez s√©lectionner une colonne num√©rique.", style={"color": "red"}), True, "Veuillez s√©lectionner une colonne num√©rique.", None
            if test_column not in numeric_columns:
                return html.P("Erreur : La colonne s√©lectionn√©e doit contenir des donn√©es num√©riques.", style={"color": "red"}), True, "La colonne doit √™tre num√©rique.", None
            data = df_temp[test_column].dropna()
            if len(data) < 3:
                return html.P("Erreur : Pas assez de donn√©es pour effectuer le test de normalit√© (au moins 3 valeurs n√©cessaires).", style={"color": "red"}), True, "Pas assez de donn√©es.", None
            stat, p_value = shapiro(data)
            results = {
                "Test": "Test de normalit√© (Shapiro-Wilk)",
                "Statistique": stat,
                "p-valeur": p_value,
                "Interpr√©tation": "Donn√©es non normales" if p_value < 0.05 else "Donn√©es normales"
            }

        elif test_type == 'chi-squared':
            if not chi2_column1 or not chi2_column2:
                return html.P("Veuillez s√©lectionner deux colonnes pour le test Chi-squared.", style={"color": "red"}), True, "Veuillez s√©lectionner deux colonnes.", None
            contingency_table = pd.crosstab(df_temp[chi2_column1], df_temp[chi2_column2])
            stat, p_value, dof, _ = chi2_contingency(contingency_table)
            results = {
                "Test": "Chi-squared",
                "Statistique": stat,
                "p-valeur": p_value,
                "Degr√© de libert√©": dof,
                "Interpr√©tation": "D√©pendance significative entre les variables" if p_value < 0.05 else "Pas de d√©pendance significative"
            }

        elif test_type == 'anova':
            if not group_column or not value_column:
                return html.P("Veuillez s√©lectionner une colonne de regroupement et une colonne de valeurs num√©riques.", style={"color": "red"}), True, "Veuillez s√©lectionner les colonnes n√©cessaires.", None
            if value_column not in numeric_columns:
                return html.P("Erreur : La colonne des valeurs doit contenir des donn√©es num√©riques.", style={"color": "red"}), True, "La colonne des valeurs doit √™tre num√©rique.", None
            groups = df_temp.groupby(group_column)[value_column].apply(lambda x: x.dropna().tolist())
            if any(len(g) < 2 for g in groups):
                return html.P("Erreur : Chaque groupe doit contenir au moins 2 observations.", style={"color": "red"}), True, "Chaque groupe doit avoir au moins 2 observations.", None
            stat, p_value = f_oneway(*groups)
            results = {
                "Test": "ANOVA",
                "Statistique": stat,
                "p-valeur": p_value,
                "Interpr√©tation": "Diff√©rence significative entre les groupes" if p_value < 0.05 else "Pas de diff√©rence significative"
            }

        # Afficher les r√©sultats sous forme de tableau
        result_df = pd.DataFrame([results])
        result_table = dbc.Table.from_dataframe(result_df, striped=True, bordered=True, hover=True, style={"color": "white"})
        return result_table, False, "Test ex√©cut√© avec succ√®s.", results

    except Exception as e:
        return html.P(f"Erreur lors de l'ex√©cution du test : {str(e)}", style={"color": "red"}), True, f"Erreur : {str(e)}", None
