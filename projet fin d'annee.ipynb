    if trigger_id == "stats-btn":
        if df.empty:
            return html.P("Veuillez charger un fichier.", style={"color": "gray"}), f"Fichier charg√©: {filename}" if filename else "Fichier charg√©: Aucun", temp_data, 'imputation'
        
